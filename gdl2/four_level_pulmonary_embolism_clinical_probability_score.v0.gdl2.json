    "rules": {
      "gt0021": {
        "id": "gt0021",
        "priority": 5,
        "when": [
          "$gt0003|Age|!=null",
          "$gt0004|Sex|!=null",
          "$gt0005|Chronic respiratory disease|!=null",
          "$gt0006|Heart rate <80|!=null",
          "$gt0007|Chest pain AND acute dyspnea|!=null",
          "$gt0008|Current estrogen use|!=null",
          "$gt0009|Prioir history of VTE|!=null",
          "$gt0010|Syncope|!=null",
          "$gt0011|Immobility within the last four weeks*|!=null",
          "$gt0012|O2 saturation <95%|!=null",
          "$gt0013|Calf pain and/or unilateral lower limb edema|!=null",
          "$gt0014|PE is the most likely diagnosis|!=null"
        ],
        "then": [
          "$gt0017|Total|.magnitude=$gt0003.value+$gt0004.value+$gt0005.value+$gt0006.value+$gt0007.value+$gt0008.value+$gt0009.value+$gt0010.value+$gt0011.value+$gt0012.value+$gt0013.value+$gt0014.value"
        ]
      },
      "gt0022": {
        "id": "gt0022",
        "priority": 4,
        "when": [
          "$gt0017|Total|<0"
        ],
        "then": [
          "$gt0019|Clinical probabilty of PE|=1|local::at0019|Very low CPP (<2%)|",
          "$gt0020|PE diagnosis|=1|local::at0024|PE can be ruled out|"
        ]
      },
      "gt0023": {
        "id": "gt0023",
        "priority": 3,
        "when": [
          "$gt0017|Total|>=0",
          "$gt0017|Total|<=5"
        ],
        "then": [
          "$gt0019|Clinical probabilty of PE|=2|local::at0020|Low CPP (2-20%)|",
          "$gt0020|PE diagnosis|=2|local::at0025|PE can be ruled out if D-dimer level <1.0 μg/mL|"
        ]
      },
      "gt0024": {
        "id": "gt0024",
        "priority": 2,
        "when": [
          "$gt0017|Total|>=6",
          "$gt0017|Total|<=12"
        ],
        "then": [
          "$gt0019|Clinical probabilty of PE|=3|local::at0021|Moderate CPP (20-65%)|",
          "$gt0020|PE diagnosis|=3|local::at0027|PE can be ruled out if D-dimer level <0.5 μg/mL OR <(age x 0.01) μg/mL|"
        ]
      },
      "gt0025": {
        "id": "gt0025",
        "priority": 1,
        "when": [
          "$gt0017|Total|>=13"
        ],
        "then": [
          "$gt0019|Clinical probabilty of PE|=4|local::at0022|High CPP (>65%)|",
          "$gt0020|PE diagnosis|=4|local::at0028|PE cannot be ruled out without imaging testing|"
        ]
      }
    }
