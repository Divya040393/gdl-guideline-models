    "rules": {
      "gt0039": {
        "id": "gt0039",
        "priority": 3,
        "when": [
          "(($gt0035|Temperature|.magnitude>38)&&($gt0035|Temperature|.unit=='°C'))||(($gt0035|Temperature|.magnitude>100.4)&&($gt0035|Temperature|.unit=='°F'))"
        ],
        "then": [
          "$gt0025|Temp >100.4 °F (38°C)|=1|local::at0067|Yes|"
        ]
      },
      "gt0041": {
        "id": "gt0041",
        "priority": 2,
        "when": [
          "$gt0033|Platelet count|<100,10*9/l"
        ],
        "then": [
          "$gt0026|Platelets <100 x 109/L|=1|local::at0073|Yes|"
        ]
      },
      "gt0038": {
        "id": "gt0038",
        "priority": 1,
        "when": [
          "$gt0003|Recent onset seizure|!=null",
          "$gt0004|Psychosis|!=null",
          "$gt0005|Organic brain syndrome|!=null",
          "$gt0006|Visual disturbance|!=null",
          "$gt0007|New onset sensory or motor neuropathy involving cranial nerves|!=null",
          "$gt0008|Lupus headache|!=null",
          "$gt0009|New onset stroke|!=null",
          "$gt0010|Vasculitis|!=null",
          "$gt0011|Arthritis|!=null",
          "$gt0012|Myositis|!=null",
          "$gt0013|Heme-granular or RBC urinary casts|!=null",
          "$gt0014|Hematuria|!=null",
          "$gt0015|Proteinuria|!=null",
          "$gt0016|Pyuria|!=null",
          "$gt0017|Inflammatory-type rash|!=null",
          "$gt0018|Alopecia|!=null",
          "$gt0019|Oral or nasal mucosal ulcers|!=null",
          "$gt0020|Pleuritic chest pain with pleural rub/effusion or pleural thickening|!=null",
          "$gt0021|Pericarditis|!=null",
          "$gt0022|Low complement|!=null",
          "$gt0023|High DNA binding|!=null",
          "$gt0030|WBC <3 x 109/L|!=null",
          "$gt0035|Temperature|!=null",
          "$gt0033|Platelet count|!=null"
        ],
        "then": [
          "$gt0028|SLEDAI-2K|.magnitude=$gt0003.value+$gt0004.value+$gt0005.value+$gt0006.value+$gt0007.value+$gt0008.value+$gt0009.value+$gt0010.value+$gt0011.value+$gt0012.value+$gt0013.value+$gt0014.value+$gt0015.value+$gt0016.value+$gt0017.value+$gt0018.value+$gt0019.value+$gt0020.value+$gt0021.value+$gt0022.value+$gt0023.value+$gt0030.value+$gt0026.value+$gt0025.value",
          "$gt0029|Comment|='Authors recommend a cutoff of 3 or 4 to define active disease and the need to increase therapy (see Evidence for details)'"
        ]
      }
