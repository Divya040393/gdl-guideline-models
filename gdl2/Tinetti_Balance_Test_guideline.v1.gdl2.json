    "default_actions": [
      "$gt0026|Balance score|=0",
      "$gt0028|Gait score|=0"
    ],
    "rules": {
      "gt0025": {
        "id": "gt0025",
        "priority": 6,
        "when": [
          "$gt0003|Sitting balance|!=null",
          "$gt0004|Rises from chair|!=null",
          "$gt0005|Attempts to rise|!=null",
          "$gt0006|Immediate standing balance|!=null",
          "$gt0007|Standing balance|!=null",
          "$gt0008|Nudged|!=null",
          "$gt0009|Eyes closed|!=null",
          "$gt0010|Turning 360 degrees - steps|!=null",
          "$gt0011|Turning 360 degrees - steadiness|!=null",
          "$gt0012|Sitting down|!=null"
        ],
        "then": [
          "$gt0026|Balance score|=$gt0003.value+$gt0004.value+$gt0005.value+$gt0006.value+$gt0007.value+$gt0008.value+$gt0009.value+$gt0010.value+$gt0011.value+$gt0012.value"
        ]
      },
      "gt0027": {
        "id": "gt0027",
        "priority": 5,
        "when": [
          "$gt0013|Indication of gait|!=null",
          "$gt0014|Step length and height - Right|!=null",
          "$gt0015|Step length and height - Left|!=null",
          "$gt0016|Foot clearance - Right|!=null",
          "$gt0017|Foot clearance - Left|!=null",
          "$gt0018|Step symmetry|!=null",
          "$gt0019|Step continuity|!=null",
          "$gt0020|Path|!=null",
          "$gt0021|Trunk|!=null",
          "$gt0022|Walking time|!=null"
        ],
        "then": [
          "$gt0028|Gait score|=$gt0013.value+$gt0014.value+$gt0015.value+$gt0016.value+$gt0017.value+$gt0018.value+$gt0019.value+$gt0020.value+$gt0021.value+$gt0022.value"
        ]
      },
      "gt0030": {
        "id": "gt0030",
        "priority": 4,
        "then": [
          "$gt0024|Tinetti Balance Score|.magnitude=$gt0026+$gt0028"
        ]
      },
      "gt0033": {
        "id": "gt0033",
        "priority": 3,
        "when": [
          "$gt0024|Tinetti Balance Score|<19"
        ],
        "then": [
          "$gt0032|Fall risk |='High'"
        ]
      },
      "gt0034": {
        "id": "gt0034",
        "priority": 2,
        "when": [
          "$gt0024|Tinetti Balance Score|>=19",
          "$gt0024|Tinetti Balance Score|<24"
        ],
        "then": [
          "$gt0032|Fall risk |='Moderate'"
        ]
      },
      "gt0035": {
        "id": "gt0035",
        "priority": 1,
        "when": [
          "$gt0024|Tinetti Balance Score|>23"
        ],
        "then": [
          "$gt0032|Fall risk |='Low'"
        ]
      }
    }
  },
