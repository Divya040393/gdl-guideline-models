    "rules": {
      "gt0018": {
        "id": "gt0018",
        "priority": 5,
        "when": [
          "$gt0005|Tone|!=null",
          "$gt0006|LOC|!=null",
          "$gt0007|Fits|!=null",
          "$gt0008|Posture|!=null",
          "$gt0009|Moro|!=null",
          "$gt0010|Grasp|!=null",
          "$gt0011|Suck|!=null",
          "$gt0012|Respiration|!=null",
          "$gt0013|Fontanelle|!=null"
        ],
        "then": [
          "$gt0015|Total score|.magnitude=$gt0005.value+$gt0006.value+$gt0007.value+$gt0008.value+$gt0009.value+$gt0010.value+$gt0011.value+$gt0012.value+$gt0013.value"
        ]
      },
      "gt0019": {
        "id": "gt0019",
        "priority": 4,
        "when": [
          "$gt0015|Total score|==0"
        ],
        "then": [
          "$gt0017|Interpretation|=$gt0020.term"
        ]
      },
      "gt0024": {
        "id": "gt0024",
        "priority": 3,
        "when": [
          "$gt0015|Total score|>=1",
          "$gt0015|Total score|<=10"
        ],
        "then": [
          "$gt0017|Interpretation|=$gt0021.term"
        ]
      },
      "gt0025": {
        "id": "gt0025",
        "priority": 2,
        "when": [
          "$gt0015|Total score|>=11",
          "$gt0015|Total score|<=14"
        ],
        "then": [
          "$gt0017|Interpretation|=$gt0022.term"
        ]
      },
      "gt0026": {
        "id": "gt0026",
        "priority": 1,
        "when": [
          "$gt0015|Total score|>=15"
        ],
        "then": [
          "$gt0017|Interpretation|=$gt0023.term"
        ]
      }
    }
  },
