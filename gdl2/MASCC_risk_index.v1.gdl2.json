    "rules": {
      "gt0019": {
        "id": "gt0019",
        "priority": 7,
        "when": [
          "$gt0003|Age|>=60,a"
        ],
        "then": [
          "$gt0035|Age (years)|=0|local::at0006|≥60|"
        ]
      },
      "gt0020": {
        "id": "gt0020",
        "priority": 6,
        "when": [
          "$gt0003|Age|<60,a"
        ],
        "then": [
          "$gt0035|Age (years)|=2|local::at0005|<60|"
        ]
      },
      "gt0021": {
        "id": "gt0021",
        "priority": 5,
        "when": [
          "$gt0005|Blood Pressure|>=90,mm[Hg]"
        ],
        "then": [
          "$gt0036|Hypotension|=5|local::at0015|No|"
        ]
      },
      "gt0022": {
        "id": "gt0022",
        "priority": 4,
        "when": [
          "$gt0005|Blood Pressure|<90,mm[Hg]"
        ],
        "then": [
          "$gt0036|Hypotension|=0|local::at0016|Yes|"
        ]
      },
      "gt0023": {
        "id": "gt0023",
        "priority": 3,
        "when": [
          "$gt0028|Status at onset of fever|!=null",
          "$gt0029|Burden of illness (symptom severity)|!=null",
          "$gt0031|Active COPD|!=null",
          "$gt0032|Type of cancer|!=null",
          "$gt0033|Dehydration requiring IV fluids|!=null",
          "$gt0035|Age (years)|!=null",
          "$gt0036|Hypotension|!=null"
        ],
        "then": [
          "$gt0037|MASCC Risk Index|.magnitude=$gt0028.value+$gt0029.value+$gt0031.value+$gt0032.value+$gt0033.value+$gt0035.value+$gt0036.value"
        ]
      },
      "gt0025": {
        "id": "gt0025",
        "priority": 2,
        "when": [
          "$gt0037|MASCC Risk Index|<21"
        ],
        "then": [
          "$gt0039|Risk for febrile neutropenia|='High risk'",
          "$gt0040|Recommendation|='Admit for empiric antibiotics if not already inpatient.'"
        ]
      },
      "gt0024": {
        "id": "gt0024",
        "priority": 1,
        "when": [
          "$gt0037|MASCC Risk Index|>=21"
        ],
        "then": [
          "$gt0039|Risk for febrile neutropenia|='Low risk'",
          "$gt0040|Recommendation|='Consider oral and/or outpatient empirical antibiotic therapy.'"
        ]
      }
    }
  },
