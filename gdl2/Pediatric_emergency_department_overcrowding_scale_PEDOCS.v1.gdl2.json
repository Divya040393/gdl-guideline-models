{
  "id": "Pediatric_emergency_department_overcrowding_scale_PEDOCS.v1",
  "gdl_version": "2.0",
  "concept": "gt0001",
  "language": {
    "original_language": "ISO_639-1::en"
  },
  "description": {
    "original_author": {
      "date": "2021-02-22",
      "name": "Therese Lindberg",
      "organisation": "Cambio CDS",
      "email": "therese.lindberg@cambio.se"
    },
    "lifecycle_state": "Initial",
    "details": {
      "en": {
        "id": "en",
        "purpose": "To calculate the PEDOCS score.",
        "keywords": [
          "PEDOCS",
          "pediatric emergency department",
          "overcrowding",
          "waiting room",
          "ED"
        ],
        "use": "Use to calculate the PEDOCS score in order to estimate severity of overcrowding in pediatrics emergency departments.\n\nScore interpretation:\nLevel:\tScore:\tInterpretation:\nLevel 1\t1-20\t        Not busy\nLevel 2\t21-60\tBusy\nLevel 3\t61-100\tExtremely busy but not overcrowded\nLevel 4\t101-140\tOvercrowded\nLevel 5\t141-180\tSeverely overcrowded\nLevel 6\t181-200\tDangerously overcrowded",
        "copyright": "Cambio CDS"
      }
    },
    "other_details": {
      "references": "1. Weiss SJ, Ernst AA, Sills MR, Quinn BJ, Johnson A, Nick TG. Development of a novel measure of overcrowding in a pediatric emergency department. Pediatr Emerg Care [Internet]. 2007 Sep [cited 2021 Feb 22];23(9):641-5. Available from: https://pubmed.ncbi.nlm.nih.gov/17876254/"
    }
  },
  "definition": {
    "data_bindings": {
      "gt0004": {
        "id": "gt0004",
        "model_id": "openEHR-EHR-EVALUATION.pedocs.v0",
        "template_id": "openEHR-EHR-EVALUATION.pedocs.v0",
        "type": "INPUT",
        "elements": {
          "gt0006": {
            "id": "gt0006",
            "path": "/data[at0001]/items[at0018]"
          },
          "gt0005": {
            "id": "gt0005",
            "path": "/data[at0001]/items[at0017]"
          }
        }
      },
      "gt0007": {
        "id": "gt0007",
        "model_id": "openEHR-EHR-EVALUATION.pedocs.v0",
        "template_id": "openEHR-EHR-EVALUATION.pedocs.v0",
        "type": "OUTPUT",
        "elements": {
          "gt0009": {
            "id": "gt0009",
            "path": "/data[at0001]/items[at0003]"
          },
          "gt0010": {
            "id": "gt0010",
            "path": "/data[at0001]/items[at0010]"
          },
          "gt0008": {
            "id": "gt0008",
            "path": "/data[at0001]/items[at0002]"
          }
        }
      }
    },
    "rules": {
      "gt0011": {
        "id": "gt0011",
        "priority": 7,
        "when": [
          "$gt0005|Total patients registered|!=null",
          "$gt0006|Number of patients in waiting room|!=null"
        ],
        "then": [
          "$gt0008|PEDOCS|.magnitude=33.3*(0.11+0.07*$gt0006.magnitude+0.04*$gt0005.magnitude)",
          "$gt0008|PEDOCS|.precision=0",
          "$gt0008|PEDOCS|.unit='1'"
        ]
      },
      "gt0017": {
        "id": "gt0017",
        "priority": 6,
        "when": [
          "$gt0008|PEDOCS|>=181,1",
          "$gt0008|PEDOCS|<=200,1"
        ],
        "then": [
          "$gt0009|Level|=local::at0009|Level 6|",
          "$gt0010|Interpretation|=local::at0016|Dangerously overcrowded|"
        ]
      },
      "gt0016": {
        "id": "gt0016",
        "priority": 5,
        "when": [
          "$gt0008|PEDOCS|>=141,1",
          "$gt0008|PEDOCS|<181,1"
        ],
        "then": [
          "$gt0009|Level|=local::at0008|Level 5|",
          "$gt0010|Interpretation|=local::at0015|Severely overcrowded|"
        ]
      },
      "gt0015": {
        "id": "gt0015",
        "priority": 4,
        "when": [
          "$gt0008|PEDOCS|>=101,1",
          "$gt0008|PEDOCS|<141,1"
        ],
        "then": [
          "$gt0009|Level|=local::at0007|Level 4|",
          "$gt0010|Interpretation|=local::at0014|Overcrowded|"
        ]
      },
      "gt0014": {
        "id": "gt0014",
        "priority": 3,
        "when": [
          "$gt0008|PEDOCS|>=61,1",
          "$gt0008|PEDOCS|<101,1"
        ],
        "then": [
          "$gt0009|Level|=local::at0006|Level 3|",
          "$gt0010|Interpretation|=local::at0013|Extremely busy but not overcrowded|"
        ]
      },
      "gt0013": {
        "id": "gt0013",
        "priority": 2,
        "when": [
          "$gt0008|PEDOCS|>=21,1",
          "$gt0008|PEDOCS|<61,1"
        ],
        "then": [
          "$gt0009|Level|=local::at0005|Level 2|",
          "$gt0010|Interpretation|=local::at0012|Busy|"
        ]
      },
      "gt0012": {
        "id": "gt0012",
        "priority": 1,
        "when": [
          "$gt0008|PEDOCS|>=1,1",
          "$gt0008|PEDOCS|<21,1"
        ],
        "then": [
          "$gt0009|Level|=local::at0004|Level 1|",
          "$gt0010|Interpretation|=local::at0011|Not busy|"
        ]
      }
    }
  },
