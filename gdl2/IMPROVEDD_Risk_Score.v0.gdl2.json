    "rules": {
      "gt0018": {
        "id": "gt0018",
        "priority": 15,
        "when": [
          "$gt0006|Previous VTE|!=null",
          "$gt0007|Known thrombophilia|!=null",
          "$gt0008|Current lower-limb paralysis|!=null",
          "$gt0009|Current cancer|!=null",
          "$gt0010|Immobilized ≥7 days*|!=null",
          "$gt0011|ICU/CCU stay|!=null",
          "$gt0012|Age >60 years|!=null",
          "$gt0013|D-dimer ≥2x ULN|!=null"
        ],
        "then": [
          "$gt0015|Total score|.magnitude=$gt0006.value+$gt0007.value+$gt0008.value+$gt0009.value+$gt0010.value+$gt0011.value+$gt0012.value+$gt0013.value"
        ]
      },
