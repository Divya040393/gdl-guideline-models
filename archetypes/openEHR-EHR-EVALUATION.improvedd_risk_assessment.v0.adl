definition
	EVALUATION[at0000] matches {    -- IMPROVEDD Risk Score assessment.
		data matches {
			ITEM_TREE[at0001] matches {    -- Item tree
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0018] occurrences matches {0..1} matches {    -- 42-day VTE risk
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0019,    -- 0.4%
									at0020,    -- 0.6%
									at0021,    -- 0.8%
									at0022,    -- 1.2%
									at0023,    -- 1.6%
									at0024]    -- 2.2%
								}
							}
						}
					}
					ELEMENT[at0025] occurrences matches {0..1} matches {    -- 77-day VTE risk
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0026,    -- 0.5%
									at0027,    -- 0.7%
									at0028,    -- 1.0%
									at0029,    -- 1.4%
									at0030,    -- 1.9%
									at0031]    -- 2.7%
								}
							}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0032] matches {*}    -- Item tree
		}
	}

