definition
	OBSERVATION[at0000] matches {    -- Lower extremity functional scale (LEFS)
		data matches {
			HISTORY[at0001] matches {    -- Event Series
				events cardinality matches {1..*; unordered} matches {
					EVENT[at0002] occurrences matches {0..*} matches {    -- Any event
						data matches {
							ITEM_TREE[at0003] matches {    -- Tree
								items cardinality matches {0..*; unordered} matches {
									ELEMENT[at0004] occurrences matches {0..1} matches {    -- Any of usual work, housework or school activities
										value matches {
											0|[local::at0005],
											1|[local::at0006],
											2|[local::at0007],
											3|[local::at0008],
											4|[local::at0009]
										}
									}
									ELEMENT[at0010] occurrences matches {0..1} matches {    -- Usual hobbies, recreational or sporting activities
										value matches {
											0|[local::at0029],
											1|[local::at0030],
											2|[local::at0031],
											3|[local::at0032],
											4|[local::at0033]
										}
									}
									ELEMENT[at0011] occurrences matches {0..1} matches {    -- Getting into or out of the bath
										value matches {
											0|[local::at0034],
											1|[local::at0035],
											2|[local::at0036],
											3|[local::at0037],
											4|[local::at0038]
										}
									}
									ELEMENT[at0012] occurrences matches {0..1} matches {    -- Walking between rooms
										value matches {
											0|[local::at0039],
											1|[local::at0040],
											2|[local::at0041],
											3|[local::at0042],
											4|[local::at0043]
										}
									}
									ELEMENT[at0013] occurrences matches {0..1} matches {    -- Putting on your shoes or socks
										value matches {
											0|[local::at0044],
											1|[local::at0045],
											2|[local::at0046],
											3|[local::at0047],
											4|[local::at0048]
										}
									}
									ELEMENT[at0014] occurrences matches {0..1} matches {    -- Squatting
										value matches {
											0|[local::at0049],
											1|[local::at0050],
											2|[local::at0051],
											3|[local::at0052],
											4|[local::at0053]
										}
									}
									ELEMENT[at0015] occurrences matches {0..1} matches {    -- Lifting an object from the floor
										value matches {
											0|[local::at0054],
											1|[local::at0055],
											2|[local::at0056],
											3|[local::at0057],
											4|[local::at0058]
										}
									}
									ELEMENT[at0016] occurrences matches {0..1} matches {    -- Performing light activities around home
										value matches {
											0|[local::at0059],
											1|[local::at0060],
											2|[local::at0061],
											3|[local::at0062],
											4|[local::at0063]
										}
									}
									ELEMENT[at0017] occurrences matches {0..1} matches {    -- Performing heavy activities around home
										value matches {
											0|[local::at0064],
											1|[local::at0065],
											2|[local::at0066],
											3|[local::at0067],
											4|[local::at0068]
										}
									}
									ELEMENT[at0018] occurrences matches {0..1} matches {    -- Getting into or out of a car
										value matches {
											0|[local::at0069],
											1|[local::at0070],
											2|[local::at0071],
											3|[local::at0072],
											4|[local::at0073]
										}
									}
									ELEMENT[at0019] occurrences matches {0..1} matches {    -- Walking 2 blocks
										value matches {
											0|[local::at0074],
											1|[local::at0075],
											2|[local::at0076],
											3|[local::at0077],
											4|[local::at0078]
										}
									}
									ELEMENT[at0020] occurrences matches {0..1} matches {    -- Walking a mile
										value matches {
											0|[local::at0079],
											1|[local::at0080],
											2|[local::at0081],
											3|[local::at0082],
											4|[local::at0083]
										}
									}
									ELEMENT[at0021] occurrences matches {0..1} matches {    -- Going up or down 10 stairs
										value matches {
											0|[local::at0084],
											1|[local::at0085],
											2|[local::at0086],
											3|[local::at0087],
											4|[local::at0088]
										}
									}
									ELEMENT[at0022] occurrences matches {0..1} matches {    -- Standing for 1 hour
										value matches {
											0|[local::at0089],
											1|[local::at0090],
											2|[local::at0091],
											3|[local::at0092],
											4|[local::at0093]
										}
									}
									ELEMENT[at0023] occurrences matches {0..1} matches {    -- Sitting for 1 hour
										value matches {
											0|[local::at0094],
											1|[local::at0095],
											2|[local::at0096],
											3|[local::at0097],
											4|[local::at0098]
										}
									}
									ELEMENT[at0024] occurrences matches {0..1} matches {    -- Running on even ground
										value matches {
											0|[local::at0099],
											1|[local::at0100],
											2|[local::at0101],
											3|[local::at0102],
											4|[local::at0103]
										}
									}
									ELEMENT[at0025] occurrences matches {0..1} matches {    -- Running on uneven ground
										value matches {
											0|[local::at0104],
											1|[local::at0105],
											2|[local::at0106],
											3|[local::at0107],
											4|[local::at0108]
										}
									}
									ELEMENT[at0026] occurrences matches {0..1} matches {    -- Making sharp turns while running fast
										value matches {
											0|[local::at0109],
											1|[local::at0110],
											2|[local::at0111],
											3|[local::at0112],
											4|[local::at0113]
										}
									}
									ELEMENT[at0027] occurrences matches {0..1} matches {    -- Hopping
										value matches {
											0|[local::at0114],
											1|[local::at0115],
											2|[local::at0116],
											3|[local::at0117],
											4|[local::at0118]
										}
									}
									ELEMENT[at0028] occurrences matches {0..1} matches {    -- Rolling over in bed
										value matches {
											0|[local::at0119],
											1|[local::at0120],
											2|[local::at0121],
											3|[local::at0122],
											4|[local::at0123]
										}
									}
									ELEMENT[at0124] occurrences matches {0..1} matches {    -- Total score
										value matches {
											DV_COUNT matches {
												magnitude matches {|0..80|}
											}
										}
									}
								}
							}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0125] matches {    -- Tree
				items cardinality matches {0..*; unordered} matches {
					allow_archetype CLUSTER[at0126] occurrences matches {0..*} matches {    -- Extension
						include
							archetype_id/value matches {/.*/}
					}
				}
			}
		}
	}

