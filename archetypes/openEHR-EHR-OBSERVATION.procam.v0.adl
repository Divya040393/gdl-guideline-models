definition
	OBSERVATION[at0000] matches {	-- Prospective Cardiovascular Münster (PROCAM) score
		data matches {
			HISTORY[at0001] matches {	-- Event Series
				events cardinality matches {1..*; unordered} matches {
					EVENT[at0002] occurrences matches {0..*} matches {	-- Any event
						data matches {
							ITEM_TREE[at0003] matches {	-- Tree
								items cardinality matches {0..*; unordered} matches {
									ELEMENT[at0004] occurrences matches {0..1} matches {	-- Age
										value matches {
											0|[local::at0005], 	--  35–39 years
											6|[local::at0006], 	-- 40-44 years
											11|[local::at0007], 	-- 45-49 years
											21|[local::at0008], 	-- 55-59 years
											26|[local::at0009]  	-- 60-65 years
										}
									}
									ELEMENT[at0010] occurrences matches {0..1} matches {	-- LDL cholesterol
										value matches {
											0|[local::at0011], 	-- <100 mg/dL
											5|[local::at0012], 	-- 100-129 mg/dL
											10|[local::at0013], 	-- 130-159 mg/dL
											14|[local::at0014], 	-- 160-189 mg/dL
											20|[local::at0015]  	-- ≥190 mg/dL
										}
									}
									ELEMENT[at0016] occurrences matches {0..1} matches {	-- HDL cholesterol
										value matches {
											0|[local::at0017], 	-- ≥55 mg/dL
											5|[local::at0018], 	-- 45-54 mg/dL
											8|[local::at0019], 	-- 35-44 mg/dL
											11|[local::at0020]  	-- <35 mg/dL
										}
									}
									ELEMENT[at0021] occurrences matches {0..1} matches {	-- Triglycerides
										value matches {
											0|[local::at0022], 	-- <100 mg/dL
											2|[local::at0023], 	-- 100–149 mg/dL
											3|[local::at0024], 	-- 150–199 mg/dL
											4|[local::at0025]  	-- ≥200  mg/dL
										}
									}
									ELEMENT[at0026] occurrences matches {0..1} matches {	-- Smoker
										value matches {
											0|[local::at0027], 	-- No
											8|[local::at0028]  	-- Yes
										}
									}
									ELEMENT[at0029] occurrences matches {0..1} matches {	-- Diabetes mellitus
										value matches {
											0|[local::at0027], 	-- No
											6|[local::at0031]  	-- Yes
										}
									}
									ELEMENT[at0032] occurrences matches {0..1} matches {	-- MI in family history
										value matches {
											0|[local::at0027], 	-- No
											4|[local::at0034]  	-- Yes
										}
									}
									ELEMENT[at0035] occurrences matches {0..1} matches {	-- Systolic blood pressure
										value matches {
											0|[local::at0036], 	-- <120 mm Hg
											2|[local::at0037], 	-- 120–129 mm Hg
											3|[local::at0038], 	-- 130–139 mm Hg
											5|[local::at0039], 	-- 140–159 mm Hg
											8|[local::at0040]  	-- ≥160 mm Hg
										}
									}
									ELEMENT[at0041] occurrences matches {0..1} matches {	-- Total score
										value matches {
											DV_COUNT matches {
												magnitude matches {|0..87|}
											}
										}
									}
									ELEMENT[at0042] occurrences matches {0..1} matches {	-- Risk of Acute Coronary Events
										value matches {
											DV_TEXT matches {*}
										}
									}
								}
							}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0043] matches {	-- Tree
				items cardinality matches {0..*; unordered} matches {
					allow_archetype CLUSTER[at0044] occurrences matches {0..*} matches {	-- Extension
						include
							archetype_id/value matches {/.*/}
					}
				}
			}
		}
	}

