definition
	EVALUATION[at0000] matches {    -- Falls Efficacy Scale (FES) score  interpretation.
		data matches {
			ITEM_TREE[at0001] matches {    -- Item tree
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0015] occurrences matches {0..1} matches {    -- Score interpretation
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::at0016]    -- FES results range from 0 to 100, where the higher the score, the lower the perceived self-efficacy at avoiding falls during non-hazardous, essential activities of daily living.
								}
							}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0014] matches {*}    -- Item tree
		}
	}

