definition
	OBSERVATION[at0000] matches {	-- Tinetti balance test
		data matches {
			HISTORY[at0001] matches {	-- Event Series
				events cardinality matches {1..*; unordered} matches {
					EVENT[at0002] occurrences matches {0..1} matches {	-- Any event
						data matches {
							ITEM_TREE[at0003] matches {	-- Tree
								items cardinality matches {0..*; unordered} matches {
									CLUSTER[at0004] occurrences matches {0..1} matches {	-- Balance Section
										items cardinality matches {1..*; unordered} matches {
											ELEMENT[at0005] occurrences matches {0..1} matches {	-- Sitting balance
												value matches {
													0|[local::at0006], 	-- Leans or slides in chair
													1|[local::at0007]  	-- Steady, safe
												}
											}
											ELEMENT[at0008] occurrences matches {0..1} matches {	-- Rises from chair
												value matches {
													0|[local::at0009], 	-- Unable to without help
													1|[local::at0010], 	-- Able, uses arms to help
													2|[local::at0011]  	-- Able without use of arms
												}
											}
											ELEMENT[at0012] occurrences matches {0..1} matches {	-- Attempts to rise
												value matches {
													0|[local::at0013], 	-- Unable to without help
													1|[local::at0014], 	-- Able, requires > 1 attempt
													2|[local::at0015]  	-- Able to rise, 1 attempt
												}
											}
											ELEMENT[at0016] occurrences matches {0..1} matches {	-- Immediate standing balance
												value matches {
													0|[local::at0017], 	-- Unsteady
													1|[local::at0018], 	-- Steady but uses walker or other support
													2|[local::at0019]  	-- Steady without walker or other support
												}
											}
											ELEMENT[at0020] occurrences matches {0..1} matches {	-- Standing balance
												value matches {
													0|[local::at0021], 	-- Unsteady
													1|[local::at0022], 	-- Steady but wide stance and uses support
													2|[local::at0023]  	-- Narrow stance without support
												}
											}
											ELEMENT[at0024] occurrences matches {0..1} matches {	-- Nudged
												value matches {
													0|[local::at0025], 	-- Begins to fall
													1|[local::at0026], 	-- Staggers, grabs, catches self
													2|[local::at0027]  	-- Steady
												}
											}
											ELEMENT[at0028] occurrences matches {0..1} matches {	-- Eyes closed
												value matches {
													0|[local::at0029], 	-- Unsteady
													1|[local::at0030]  	-- Steady
												}
											}
											ELEMENT[at0031] occurrences matches {0..1} matches {	-- Turning 360 degrees - steps
												value matches {
													0|[local::at0032], 	-- Discontinuous steps
													1|[local::at0033]  	-- Continuous
												}
											}
											ELEMENT[at0034] occurrences matches {0..1} matches {	-- Turning 360 degrees - steadiness
												value matches {
													0|[local::at0035], 	-- Unsteady
													1|[local::at0036]  	-- Steady
												}
											}
											ELEMENT[at0037] occurrences matches {0..1} matches {	-- Sitting down
												value matches {
													0|[local::at0038], 	-- Unsafe 
													1|[local::at0039], 	-- Uses arms or not a smooth motion
													2|[local::at0040]  	-- Safe, smooth motion
												}
											}
										}
									}
									CLUSTER[at0041] occurrences matches {0..1} matches {	-- Gait Section
										items cardinality matches {1..*; unordered} matches {
											ELEMENT[at0042] occurrences matches {0..1} matches {	-- Indication of gait
												value matches {
													0|[local::at0043], 	-- Any hesitancy or multiple attempts
													1|[local::at0044]  	-- No hesitancy
												}
											}
											ELEMENT[at0045] occurrences matches {0..1} matches {	-- Step length and height - Right
												value matches {
													0|[local::at0046], 	-- Step to
													1|[local::at0047]  	-- Step through R
												}
											}
											ELEMENT[at0048] occurrences matches {0..1} matches {	-- Step length and height - Left
												value matches {
													0|[local::at0049], 	-- Step to
													1|[local::at0050]  	-- Step through L
												}
											}
											ELEMENT[at0051] occurrences matches {0..1} matches {	-- Foot clearance - Right
												value matches {
													0|[local::at0052], 	-- Foot drop
													1|[local::at0053]  	-- R foot clears floor
												}
											}
											ELEMENT[at0054] occurrences matches {0..1} matches {	-- Foot clearance - Left
												value matches {
													0|[local::at0055], 	-- Foot drop
													1|[local::at0056]  	-- L foot clears floor
												}
											}
											ELEMENT[at0057] occurrences matches {0..1} matches {	-- Step symmetry
												value matches {
													0|[local::at0058], 	-- Right and left step lengths not equal
													1|[local::at0059]  	-- Right and left step lengths appear equal
												}
											}
											ELEMENT[at0060] occurrences matches {0..1} matches {	-- Step continuity
												value matches {
													0|[local::at0061], 	-- Stopping or discontinuity between steps
													1|[local::at0062]  	-- Steps appear continuous
												}
											}
											ELEMENT[at0063] occurrences matches {0..1} matches {	-- Path
												value matches {
													0|[local::at0064], 	-- Marked deviation
													1|[local::at0065], 	-- Mild/moderate deviation or uses w. aid
													2|[local::at0066]  	-- Straight without w. aid
												}
											}
											ELEMENT[at0067] occurrences matches {0..1} matches {	-- Trunk
												value matches {
													0|[local::at0068], 	-- Marked sway or uses w. aid
													1|[local::at0069], 	-- No sway but flex. knees or back or uses arms for stability
													2|[local::at0070]  	-- No sway, flex., use of arms or w. aid
												}
											}
											ELEMENT[at0071] occurrences matches {0..1} matches {	-- Walking time
												value matches {
													0|[local::at0072], 	-- Heels apart
													1|[local::at0073]  	-- Heels almost touching while walking
												}
											}
										}
									}
									ELEMENT[at0074] occurrences matches {0..1} matches {	-- Total score
										value matches {
											DV_COUNT matches {
												magnitude matches {|0..28|}
											}
										}
									}
								}
							}
						}
					}
				}
			}
		}
	}

